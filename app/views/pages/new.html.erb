<p id="notice"><%= notice %></p>

<%= form_tag login_path do %>

		<div class="field">
			<label for="email">Email</label>
			<%= text_field_tag :email %>
		</div>

		<div class="field">
			<label for="password">password</label>
			<%= password_field_tag :password %>
		</div>

		<div class="actions">
			<input class="admin-btn" type="submit" value="Log In">
		</div>
<% end %>